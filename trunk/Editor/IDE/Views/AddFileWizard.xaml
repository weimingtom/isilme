<Window x:Class="IDE.Views.NewFileWizard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="NewFileWizard" Height="300" Width="613">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="162*" />
            <RowDefinition Height="59*" />
            <RowDefinition Height="40*" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="410*" />
            <ColumnDefinition Width="181*" />
        </Grid.ColumnDefinitions>
        <ListView Name="mFileTypeList" />
        <TextBox Grid.Row="1" Height="23" HorizontalAlignment="Left" Margin="272,6,0,0" Name="mFileName" VerticalAlignment="Top" Width="130" />
    </Grid>
</Window>
